<div id="<%= dom_id event %>">
  <p>
    <strong>Name:</strong>
    <%= event.name %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= event.price %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= event.description %>
  </p>

  <p>
    <strong>Seller:</strong>
    <%= event.seller_id %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= event.date %>
  </p>

</div>
